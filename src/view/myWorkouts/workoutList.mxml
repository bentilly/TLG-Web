<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 
		 width="100%"
		 height="100%"
		 
		 >
	
	<fx:Script>
		<![CDATA[
			import mx.collections.ArrayCollection;
			
			/*** IMPORTED ***/
			[Bindable] public var workout_collection:ArrayCollection;
			
			
		]]>
	</fx:Script>
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	
	<s:Line height="100%">
		<s:stroke>
			<s:SolidColorStroke color="#cccccc" weight="1"/>
		</s:stroke>
	</s:Line>
	
	<s:DataGroup dataProvider="{workout_collection}" itemRenderer="view.skinsAndRenderers.workoutListRenderer" rollOverColor="#ffffff">
		<s:layout>
			<s:VerticalLayout gap="0"/>
		</s:layout>
	</s:DataGroup>
	
	
</s:Group>
