<?xml version="1.0" encoding="utf-8"?>
<s:Group xmlns:fx="http://ns.adobe.com/mxml/2009" 
		 xmlns:s="library://ns.adobe.com/flex/spark" 
		 xmlns:mx="library://ns.adobe.com/flex/mx"
		 xmlns:dataViz="view.dataViz.*"
		 
		 height="100%"
		 
		 >
	
	<fx:Script>
		<![CDATA[
			
			/*** IMPORTED ***/
			[Bindable] public var userName:String;
			
		]]>
	</fx:Script>
	
	
	<fx:Declarations>
		<!-- Place non-visual elements (e.g., services, value objects) here -->
	</fx:Declarations>
	
	<s:BorderContainer backgroundAlpha="0.1" backgroundColor="#ffffff" width="100%" height="100%" borderVisible="false">
		<s:Line height="100%" right="0" alpha=".2">
			<s:stroke>
				<s:SolidColorStroke color="0xffffff" weight="1"/>
			</s:stroke>
		</s:Line>
		
		<s:VGroup horizontalCenter="0" verticalCenter="0" height="450">
			<s:Label id="myTrainingLabel" text="My Workouts" styleName="h1_white" paddingBottom="20"/>
			<s:Image source="@Embed(source='../../assets/angrymonkey.png')"/>
			<s:Label text="{userName}" styleName="h2_white" width="200" height="50"/>
			<dataViz:smallDailyCol/>
		</s:VGroup>
		
	</s:BorderContainer>
	
	
	
	
	
	
</s:Group>
